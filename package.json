{
  "name": "nostalgie",
  "version": "0.60.0",
  "description": "Opinionated template repository for creating a TypeScript library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nostalgie": "bin/nostalgie"
  },
  "files": [
    "dist/**/!(tsconfig.tsbuildinfo)",
    "runtime/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:cli": "node ./scripts/buildCli.js",
    "build:stub": "node ./scripts/buildStub.js",
    "build:types": "node ./scripts/buildTypes.js",
    "build": "npm-run-all --silent clean --parallel build:*",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/hapi-pino": "^8.0.1",
    "@types/hapi__inert": "^5.2.2",
    "@types/node": "14",
    "@types/pino": "^6.3.4",
    "@types/react": "16",
    "@types/react-dom": "16",
    "@types/react-router-dom": "5",
    "@types/react-side-effect": "^1.1.1",
    "@types/workerpool": "^6.0.0",
    "abort-controller": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggoodman/typescript-library-template.git"
  },
  "keywords": [],
  "author": "Geoffrey Goodman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ggoodman/typescript-library-template/issues"
  },
  "homepage": "https://github.com/ggoodman/typescript-library-template#readme",
  "engines": {
    "node": ">12.2.0"
  },
  "engineStrict": true,
  "prettier": {
    "arrowParens": "always",
    "printWidth": 100,
    "singleQuote": true
  },
  "dependencies": {
    "@hapi/boom": "^9.1.1",
    "@hapi/hapi": "^20.0.3",
    "@hapi/inert": "^6.0.3",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "@twind/typography": "0.0.1",
    "chokidar": "^3.5.0",
    "dataloader": "^2.0.0",
    "esbuild": "^0.8.31",
    "escape-string-regexp": "^4.0.0",
    "hapi-pino": "^8.3.0",
    "joi": "^17.3.0",
    "markdown-to-jsx": "^7.1.0",
    "pino": "^6.8.0",
    "pino-pretty": "^4.3.0",
    "piscina": "^2.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-error-overlay": "^6.0.8",
    "react-head": "^3.4.0",
    "react-helmet-async": "^1.0.7",
    "react-query": "^3.5.11",
    "react-router-dom": "^5.2.0",
    "react-ssr-prepass": "^1.2.1",
    "twind": "^0.11.1",
    "workerpool": "^6.0.3",
    "yargs": "^16.2.0"
  }
}
